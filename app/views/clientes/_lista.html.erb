<tr id="cliente_<%= cliente.id %>">
			<!-- ><th scope="row" class="table-check-cell"><input type="checkbox" name="selected[]" id="table-selected-1" value="1"></th> -->
			<td><%= cliente.id %></td>
			<td>
			<ul class="keywords">
				<li>
					<%= cliente.tipo %>
				</li>
			</ul></td>
			<td><%= cliente.nome %></td>
			<td><%= cliente.endereco %></td>
			<td><%= cliente.ddd + ' - ' + cliente.telefone %></td>
			<td><%= cliente.nome_contato %></td>
			
			<% if cliente.historico_de_contatos.any? %>
			<% @message = "Esse cliente nÃ£o pode ser apagado." %>
			<% else %>
			<% @message = ' Tem certeza que deseja apagar o cliente ' + cliente.nome + '?' %>
			<% end %>	
			<!-- The class table-actions is designed for action icons -->
			<td class="table-actions"> <%= link_to image_tag("/images/icons/fugue/application-blog.png"), cliente, :class => "with-tip", :title =>"Visualizar", :remote => true %>
			<%= link_to image_tag("/images/icons/fugue/pencil.png"), edit_cliente_path(cliente), :title =>"Edit", :class => "with-tip", :remote => true %>
			<%= link_to image_tag("/images/icons/fugue/cross-circle.png"), cliente, confirm: @message , method: :delete, :title =>"Delete", :class => "with-tip", :remote => true %> </td>
		</tr>