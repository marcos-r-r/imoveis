    <td><%= link_to 'Show', historico_de_contato %></td>
    <td><%= link_to 'Edit', edit_historico_de_contato_path(historico_de_contato) %></td>
    <td><%= link_to 'Destroy', historico_de_contato, confirm: 'Are you sure?', method: :delete %></td>
